{
  "name": "lumitek-backend-express",
  "version": "0.0.0",
  "private": true,
  "_moduleAliases": {
    "@/controller": "src/controller",
    "@/service": "src/service"
  },
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "format": "prettier --write ."
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "mssql": "^10.0.2",
    "multer": "^1.4.5-lts.1",
    "node-xlsx": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.4",
    "nodemon": "^3.1.4"
  }
}
